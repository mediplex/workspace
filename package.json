{
  "nyame": "workspaces",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn --cwd packages/core dev",
    "build": "yarn --cwd packages/core build",
    "start": "yarn --cwd packages/core start",
    "commit": "git-cz"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "firebase-tools": "^8.4.1",
    "prettier": "^2.0.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {}
}